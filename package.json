{
  "name": "snake-wars",
  "version": "0.1.0",
  "description": "Snake wars boot gaming playing platform",
  "main": "index.js",
  "scripts": {
    "start": "yarn backend-start",
    "frontend-start": "cd frontend && yarn start",
    "backend-start": "cd backend && yarn start",
    "watch": "yarn backend-watch & yarn frontend-watch",
    "frontend-watch": "cd frontend && yarn watch",
    "backend-watch": "cd backend && yarn watch",
    "test": "yarn frontend-test",
    "frontend-test": "cd frontend && yarn test",
    "build": "yarn frontend-build && yarn backend-build",
    "frontend-build": "cd frontend && yarn build",
    "backend-build": "cd backend && yarn build",
    "frontend-install": "cd frontend && yarn install",
    "backend-install": "cd backend && yarn install",
    "postinstall": "yarn frontend-install && yarn backend-install && yarn build",
    "seed": "./seed.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrostF0X/snake_wars.git"
  },
  "author": "frost_fox",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/syzygypl/snake-wars/issues"
  },
  "homepage": "https://github.com/syzygypl/snake-wars#readme",
  "dependencies": {
    "express": "^4.16.3"
  }
}
